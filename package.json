{
  "name": "shoppe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "format": "eslint ./src --fix && stylelint '**/*.scss' --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "lint:ts": "eslint ./src",
    "lint:css": "stylelint '**/*.scss'",
    "lint:fsd": "steiger ./src",
    "test": "vitest --config src/shared/config/vitest/vitest.config.ts --run",
    "test:watch": "vitest --config src/shared/config/vitest/vitest.config.ts"
  },
  "dependencies": {
    "@bem-react/classname": "1.6.0",
    "next": "15.1.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sass": "1.84.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "20",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/testing-library__jest-dom": "6.0.0",
    "@typescript-eslint/parser": "8.24.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-unused-imports": "4.1.4",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.3",
    "prettier": "3.5.1",
    "steiger": "0.5.5",
    "stylelint": "16.14.1",
    "stylelint-config-clean-order": "7.0.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "typescript": "5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8",
    "yarn": "1.22.22"
  }
}
